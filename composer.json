{
    "name": "asyncswarm/curl",
    "description": "cURL wrapper for PHP. Supports parallel and non-blocking requests. For high speed crawling, see stil/curl-robot.",
    "license": "MIT",
    "minimum-stability": "stable",
    "type": "library",
    "require": {
        "symfony/event-dispatcher": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "cURL\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "cURL\\Tests\\": "tests/"
        }
    }
}
